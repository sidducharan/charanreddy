const conversationHistory = document.getElementById('conversation-history');
const messageInputField = document.getElementById('message-input-field');
const sendMessageButton = document.getElementById('send-message-button');


function displayMessage(message, isUserMessage = false) {
  const messageElement = document.createElement('p');
  const messageContainer = document.createElement('div');

  if (isUserMessage) {
    messageElement.classList.add('user-message');
    messageContainer.classList.add('user-message-container'); 
  } else {
    messageContainer.classList.add('chatbot-message-container'); 
  }

  messageElement.textContent = message;
  messageContainer.appendChild(messageElement); 
  conversationHistory.appendChild(messageContainer);
}

sendMessageButton.addEventListener('click', async () => {
  const userMessage = messageInputField.value.trim();

  if (userMessage === '') {
    alert("Please enter a message.");
    return;
  }

  // Display user message
  displayMessage(`You: ${userMessage}`, true);

  // Simulate chatbot response (Replace with your actual chatbot logic or API integration)
  let chatbotResponse;

  switch (userMessage.toLowerCase()) {
    case 'hello':
    case 'hi':
      chatbotResponse = "Hi there! How can I assist you today?";
      break;
    case 'admission':
      chatbotResponse = "For admission inquiries, please visit our admissions page or contact the admissions office at [phone number or email]";
      break;
    case 'karthik is an idiot' :
        chatbotResponse ="yes karthik is an mf";
        break;
    case 'tell me admission for aspire academy':
      chatbotResponse = "FFor admission inquiries, please visit our admissions page or contact the admissions office at [phone number or email]";
      break;
      case 'courses':
      chatbotResponse = "We offer a variety of programs. Please visit our courses page for a complete list.";
      break;
    case 'fees':
      chatbotResponse = "For information on tuition fees and other costs, please refer to our fee structure page.";
      break;
    case 'contact':
      chatbotResponse = "You can contact us at [phone number] or [email address].";
      break;
    default:
      chatbotResponse = "I'm still learning. I'm not sure how to answer that yet.";
  }

  // Add a slight delay to simulate real-time response
  await new Promise(resolve => setTimeout(resolve, 500)); 

  displayMessage(`Chatbot: ${chatbotResponse}`);

  // Scroll to the bottom of the conversation history
  conversationHistory.scrollTop = conversationHistory.scrollHeight;

  messageInputField.value = ''; // Clear the input field
});
const chatbotWidget = document.getElementById('chatbot-widget');

chatbotWidget.addEventListener('click', () => {
  chatbotWidget.classList.toggle('active'); 
});
