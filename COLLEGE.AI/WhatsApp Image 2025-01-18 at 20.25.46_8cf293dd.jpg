/* General Styles */
body {
  font-family: sans-serif;
  margin: 0;
}

header {
  background-color: #333;
  color: #fff;
  padding: 1em 0;
  text-align: center;
}

nav {
  background-color: #f0f0f0;
  padding: 1em 0;
  text-align: center;
}

nav a {
  color: #333;
  text-decoration: none;
  margin: 0 1em;
}

.container {
  max-width: 960px;
  margin: 0 auto;
  padding: 20px;
}

.circle-image {
  display: inline-block;
  margin: 0 auto;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  overflow: hidden;
}

.circle-image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

/* Text Animations */

h1,
h2,
.hero p {
  animation: fadeInUp 1s ease-in-out;
}

@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

p {
  animation: fadeIn 1s ease-in-out;
}

@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

/* Hero Section */

.hero {
  text-align: center;
  padding: 2em 0;
}

.hero marquee {
  background-color: #f0f0f0;
  color: red;
  font-weight: bold;
  padding: 5px;
}

/* About Us Section */

#about {
  margin-bottom: 2em;
}

/* Courses Section */

#courses ul {
  list-style: none;
  padding: 0;
  margin: 0;
}

#courses li {
  margin-bottom: 1em;
}

/* Contact Section */

#contact p {
  margin-bottom: 0.5em;
}

/* Admissions Section */

.admissions a {
  background-color: #333;
  color: #fff;
  padding: 0.5em 1em;
  border-radius: 5px;
  text-decoration: none;
}

.admissions a:hover {
  background-color: #444;
}

/* Slider (assuming this is in a separate script.js file) */

.slider {
  overflow: hidden;
  position: relative;
}

.slider img {
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  opacity: 0;
  transition: opacity 0.5s ease-in-out;
}

.slider img:first-child {
  opacity: 1;
}
#chatbot-widget {
  position: fixed;
  bottom: 20px;
  right: 20px;
  width: 300px;
  background-color: #fff;
  border-radius: 10px;
  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
  padding: 15px;
}

#conversation-history {
  height: 200px;
  overflow-y: auto;
  padding: 10px;
}

.message-container {
  margin-bottom: 5px;
  padding: 8px;
  border-radius: 5px;
}

.user-message-container {
  background-color: #e6f7ff;
  text-align: right;
}

.chatbot-message-container {
  background-color: #e0f2e0;
  text-align: left;
}

.message {
  font-size: 14px;
}

.message-input {
  display: flex;
  margin-top: 10px;
}

#message-input-field {
  flex: 1;
  padding: 8px;
  border: 1px solid #ccc;
  border-radius: 5px;
  margin-right: 5px;
}

#send-message-button {
  padding: 8px 12px;
  background-color: #007bff;
  color: #fff;
  border: none;
  border-radius: 5px;
  cursor: pointer;
}

